{
        "name": "voxelized-js",
        "version": "0.1.0",
        "author": "tseijp",
        "license": "MIT",
        "private": false,
        "sideEffect": false,
        "type": "module",
        "publishConfig": {
                "access": "public"
        },
        "bugs": {
                "url": "https://github.com/tseijp/voxelizer/issues"
        },
        "repository": "tseijp/voxelizer",
        "homepage": "https://voxelizer.tsei.jp",
        "keywords": [
                "glsl",
                "webgl",
                "webgpu",
                "voxel",
                "voxeler",
                "typescript"
        ],
        "main": "./dist/index.cjs",
        "types": "./dist/index.d.ts",
        "module": "./dist/index.js",
        "exports": {
                "./packages.json": "./packages.json",
                "./src": {
                        "types": "./src/index.ts",
                        "import": "./src/index.ts",
                        "default": "./src/index.ts"
                },
                ".": {
                        "types": "./dist/index.d.ts",
                        "import": {
                                "types": "./dist/index.d.ts",
                                "default": "./dist/index.js"
                        },
                        "module": {
                                "types": "./dist/index.d.ts",
                                "default": "./dist/index.js"
                        },
                        "require": "./dist/index.cjs",
                        "default": "./dist/index.js"
                },
                "./*": {
                        "types": "./dist/index.d.ts",
                        "import": {
                                "types": "./dist/index.d.ts",
                                "default": "./dist/index.js"
                        },
                        "module": {
                                "types": "./dist/index.d.ts",
                                "default": "./dist/index.js"
                        },
                        "require": "./dist/index.cjs",
                        "default": "./dist/index.js"
                }
        },
        "files": [
                "dist/**/*",
                "src/**/*",
                "packages.json",
                "README.md"
        ],
        "dependencies": {
                "gl-matrix": "*"
        }
}
